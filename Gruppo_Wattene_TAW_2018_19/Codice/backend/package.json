{
  "name": "backend",
  "version": "1.1.0",
  "description": "backend for our application",
  "main": "dist/index.js",
  "engines": {
    "node": "13.7.0",
    "npm": "6.13.6"
  },
  "scripts": {
    "lint": "eslint src/** --max-warnings 10000 --fix-dry-run",
    "build": "tsc",
    "watch": "ts-node-dev src/index.ts",
    "localDB": "concurrently \"mongod --dbpath ./data\" \"node .\"",
    "watchlocalDB": "concurrently \"mongod --dbpath ./data\" \"ts-node-dev src/index.ts\"",
    "prestart": "",
    "start": "npm run env NODE_ENV=production&& ts-node-dev src/index.ts",
    "fastStart": "node .",
    "precleanstart": "node cleaner.js",
    "cleanstart": "npm start",
    "pretest": "npm run build",
    "test": "mocha --exit",
    "fasTest": "mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:unive11/taw/backend.git"
  },
  "author": "WATTENE",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-query-boolean": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.9",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "rootpath": "^0.1.2",
    "socket.io": "^2.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.17.1",
    "@types/express-jwt": "^0.0.42",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.31",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.7.1",
    "@types/passport": "^1.0.1",
    "@types/socket.io": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "chai": "^4.2.0",
    "chai-http": "4.3.0",
    "eslint": "^6.4.0",
    "mocha": "^6.2.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.5.3"
  }
}
